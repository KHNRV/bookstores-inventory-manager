{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./server.js localhost 3001",
    "db:migrate": "knex migrate:latest",
    "db:unmigrate": "knex migrate:rollback",
    "db:seed": "knex seed:run ",
    "db:reset": "knex migrate:rollback && knex migrate:latest && knex seed:run",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "morgan": "~1.9.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
